# Hamada Tool Environment Variables
# Copy this file to .env and fill in your actual values

# Required - Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional - AI Service APIs (for future enhancements)
ANTHROPIC_API_KEY=sk-ant-api03-your-key-here
ELEVENLABS_API_KEY=sk-your-elevenlabs-key-here
GEMINI_API_KEY=AIzaSy-your-gemini-key-here
OPENAI_API_KEY=sk-proj-your-openai-key-here

# Optional - Google OAuth (for future authentication)
GOOGLE_OAUTH_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET=your-google-client-secret

# Optional - Google Service Account (for advanced features)
GOOGLE_SERVICE_ACCOUNT_KEY={"type": "service_account", "project_id": "your-project", ...}

# Optional - Twilio (for SMS notifications)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Optional - Analytics
VITE_GA_MEASUREMENT_ID=G-your-ga-id